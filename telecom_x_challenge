        "\n",
        "def comprobar_nulos(datos):\n",
        "  for columna in datos.columns:\n",
        "    valores_nulos = datos[columna].isnull().sum()\n",
        "    print(f\"Valores nulos en la columna '{columna}':\")\n",
        "    print(valores_nulos)\n",
        "    print()"
